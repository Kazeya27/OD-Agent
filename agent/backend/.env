DB_PATH=./geo_points.db
TABLE_PLACES=places
TABLE_RELATIONS=relations
TABLE_DYNA=dyna
PORT=8000

# LLM 配置
# 选择 LLM 提供商: gemini 或 qwen
LLM_PROVIDER=qwen
# Gemini 模型: gemini-2.5-flash-preview-05-20, gemini-1.5-pro, etc.
# Qwen 模型: qwen-turbo, qwen-plus, qwen-max, qwen-max-longcontext
LLM_MODEL=qwen-plus
LLM_TEMPERATURE=0.6

# 后端服务地址
BASE_URL=http://127.0.0.1:8000

# Agent 服务端口
AGENT_PORT=8001

# API Keys
# Google API Key (用于 Gemini)
GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY

# Qwen API Key (用于通义千问)
# 可以使用 QWEN_API_KEY 或 DASHSCOPE_API_KEY
QWEN_API_KEY=YOUR_QWEN_API_KEY
# DASHSCOPE_API_KEY=YOUR_DASHSCOPE_API_KEY
# Qwen API Base URL (默认为阿里云 DashScope)
QWEN_API_BASE=https://dashscope.aliyuncs.com/compatible-mode/v1
